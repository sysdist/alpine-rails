{
  "name": "Rails app",
  "version": "0.3",
  "slug": "rails_app",
  "description": "My first real add-on in Rails!",
  "startup": "application",
  "boot": "auto",
  "homeassistant_api": true,
  "options": {
    "message": "custom things",
    "logins": [
      { "username": "beer", "password": "123456" },
      { "username": "cheep", "password": "654321" }
