  "schema": {
    "message": "str",
    "logins": [
      { "username": "str", "password": "str" }
    ],
    "link": "url",
    "size": "int(5,20)",
    "count": "float",
    "target": "str",
    "not_need": "str?"
  },
  "ports": {
    "3000/tcp": 3000
  }
  
}